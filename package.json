{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "eslint": "eslint .",
    "webpack": "webpack -r babel-register",
    "hot-server": "node -r babel-register dev-server",
    "hot-start": "electron -r babel-register ./main",
    "build_all": "electron-packager ./ MyElectronApp --platform=all --arch=x64 --out ./dist --version 1.3.5 --asar=true --overwrite --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ignore=dist --prune",
    "build_linux": "electron-packager ./ MyElectronApp --platform=linux --arch=x64 --out ./dist --version 1.3.5 --asar=true --overwrite --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ignore=dist --prune",
    "build_win32": "electron-packager ./ MyElectronApp --platform=win32 --arch=x64 --out ./dist --version 1.3.5 --asar=true --overwrite --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ignore=dist --prune",
    "build_mac": "electron-packager ./ MyElectronApp --platform=darwin --arch=x64 --out ./dist --version 1.3.5 --asar=true --overwrite --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ignore=dist --prune",
    "test": "mocha --compilers js:babel-core/register ./test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/electron-quick-start.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/electron/electron-quick-start/issues"
  },
  "homepage": "https://github.com/electron/electron-quick-start#readme",
  "dependencies": {
    "react": "^0.14.8",
    "react-dom": "^0.14.8"
  },
  "devDependencies": {
    "asar": "^0.12.3",
    "babel-core": "^6.0.20",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "electron": "^1.3.5",
    "electron-packager": "^7.7.0",
    "electron-prebuilt": "^1.3.5",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^6.2.1",
    "mocha": "^3.0.2",
    "react-hot-loader": "^1.3.0",
    "spectron": "^3.3.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "webpack-target-electron-renderer": "^0.4.0"
  }
}
